{
  "name": "Google Search Keyboard Shortcuts",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Google Search Keyboard Shortcuts",
  "content_scripts": [
    {
      "matches": [
        "http://*.google.com/search?*",
        "http://*.google.com/",
        "https://*.google.com/search?*",
        "https://*.google.com/"
      ],
      "js": ["jquery-1.7.2.min.js", "keymaster.min.js", "script.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}

